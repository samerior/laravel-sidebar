{
    "name": "samerior/sidebar",
    "type": "library",
    "description": "A Laravel Sidebar generator",
    "keywords": [
        "samerior",
        "sidebar"
    ],
    "homepage": "https://github.com/samerior/sidebar",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Dervis",
            "email": "sam@samerior.com",
            "homepage": "https://www.samerior.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Samerior\\LaravelSidebar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Samerior\\LaravelSidebar\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
